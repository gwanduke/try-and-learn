{
  "name": "네트워크 캡쳐",
  "description": "네트워크 캡쳐",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["webRequest"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "./options.html",
  "host_permissions": ["*://*/*"]
}
